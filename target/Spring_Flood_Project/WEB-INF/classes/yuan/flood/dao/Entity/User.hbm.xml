<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="yuan.flood.dao.Entity">
    <class name="User" table="My">
        <id name="userLonID" type="java.lang.Long" column="id">
            <generator class="native">
            </generator>
        </id>
        <property name="userID" type="java.lang.String" column="user_id"></property>
        <property name="passWord" type="java.lang.String" column="password"></property>
    </class>
</hibernate-mapping>