<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="yuan.flood.dao.Entity">
    <class name="PredictWaterLevelResult" table="fpredictRes">
        <id name="id" type="java.lang.Long" column="predict_id">
            <generator class="native"/>
        </id>
        <property name="predictTime" type="java.util.Date" column="predict_time"/>
        <property name="timeLonMatrixStr" type="text"  column="time_data"/>
        <property name="predictResultMatrixStr" type="text" column="predict_data"></property>
        <property name="trainTargetMatrixStr" type="text" column="real_data"></property>
        <many-to-one name="subscibeEventParams" lazy="false" class="SubscibeEventParams" cascade="save-update" column="ses_id"></many-to-one>
    </class>
</hibernate-mapping>